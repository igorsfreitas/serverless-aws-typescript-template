{
  "name": "core-app-ts",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "create:module": "npx ts-node src/scripts/create-module/create-module.ts",
    "dev": "npx sls offline start --host 0.0.0.0 --reloadHandler",
    "deploy:local": "sls deploy --stage local",
    "test:changed": "npm run test -- --onlyChanged",
    "test": "npx jest --noStackTrace --runInBand --detectOpenHandles --passWithNoTests --silent",
    "test:verbose": "npx jest --noStackTrace --runInBand --detectOpenHandles --passWithNoTests --verbose",
    "test:staged": "npm run test -- --bail --findRelatedTests",
    "test:unit": "npm run test -- --watch -c jest-unit-config.js",
    "test:integration": "npm run test -- --watch -c jest-integration-config.js",
    "test:ci": "npm run test -- --coverage",
    "lint": "npx eslint --fix --ignore-path .eslintignore --ext .js,.ts .",
    "prepare": "husky install",
    "clean": "rm -rf node_modules & rm -rf .esbuild; wait"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.454.0",
    "@middy/core": "^4.6.0",
    "@middy/http-cors": "^4.6.0",
    "@middy/http-json-body-parser": "^4.6.0",
    "@middy/http-security-headers": "^4.6.0",
    "@types/redis": "^4.0.11",
    "aws-sdk": "^2.1450.0",
    "axios": "^1.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "lodash.omit": "^4.5.0",
    "md5": "^2.3.0",
    "middle": "^0.0.2",
    "pg": "^8.11.3",
    "pg-promise": "^11.5.4",
    "querystring": "^0.2.1",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "uuid": "^9.0.1",
    "uuidv4": "^6.2.13",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.5.2",
    "@types/lodash.omit": "^4.5.7",
    "@types/md5": "^2.3.2",
    "@types/node": "^14.18.56",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "esbuild": "^0.14.11",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-mock-extended": "^3.0.1",
    "json-schema-to-ts": "^1.5.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^12.0.4",
    "serverless-offline-schedule": "^1.0.4",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "igorsfreitas",
  "license": "MIT"
}
